{
  "name": "happystoreshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && cp _routes.json out/_routes.json",
    "start": "next start",
    "lint": "eslint",
    "generate-images": "tsx scripts/generate-images.ts",
    "preview": "npm run build && npx wrangler pages dev out/ --port 8788",
    "deploy": "npm run build && npx wrangler pages deploy out/ --project-name happystoreshop-ecommerce --commit-dirty=true",
    "typecheck:functions": "tsc --project tsconfig.functions.json --noEmit"
  },
  "dependencies": {
    "lucide-react": "^0.564.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
